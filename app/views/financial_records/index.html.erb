<h1>FinancialRecords#index</h1>
<p>Find me in app/views/financial_records/index.html.erb</p>


<div class="dashboard_form-inner_section">
  <%= simple_form_for :financial_record, url: import_financial_records_path, html: { multipart: true } do |f| %>
    <%= f.error_notification %>
    <%= f.error_notification message: f.object.errors[:base].to_sentence if f.object.errors[:base].present? %>

    <div class="form-inputs">
      <div><%= f.input :id_number, label: false %></div>
    </div>
  <% end %>
</div>
